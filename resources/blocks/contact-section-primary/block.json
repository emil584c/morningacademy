        "viewportWidth": 1920,
        "innerBlocks": [
            {
                "name": "mt/tagline",
                "attributes": {
                    "content": "Tøv ikke - vi er altid klar på en snak"
                }
            },
            {
                "name": "mt/heading",
                "attributes": {
                    "content": "Lad os tage en snak om dit digitale projekt"
                }
            },
            {
                "name": "core/paragraph",
                "attributes": {
                    "content": "Lad os gå i detaljer så vi sammen kan finde løsninger, der passer til dine behov."
                }
            },
            {
                "name": "mt/buttons",
                "innerBlocks": [
                    {
                        "name": "mt/button",
                        "attributes": {
                            "text": "Kontakt os",
                            "type": "secondary"
                        }
                    },
                    {
                        "name": "mt/button",
                        "attributes": {
                            "text": "Book et møde",
                            "type": "calendar"
                        }
                    }
                ]
            }
        ]
    },
    "providesContext": {
        "button/types": "buttonTypes"
